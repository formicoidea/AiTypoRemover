{
  "name": "AI text cleaner",
  "version": "0.0",
  "description": "An extension that removes typos gernerated by AI in text outputs.",
  "manifest_version": 3,
  "permissions": [
    "contextMenus",
    "scripting",
    "clipboardWrite",
    "activeTab",
    "storage"
 ],
 "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/icon128.png",
      "128": "icons/icon326.jpg"
    }
  },
  "icons": {
      "48": "icons/icon128.png",
      "128": "icons/icon326.jpg"
   }
}